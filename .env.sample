# URL do Supabase ou URL do seu banco de dados PostgreSQL
SUPABASE_URL=https://your-supabase-url.supabase.co

# Chave de API Anônima do Supabase
SUPABASE_ANON_KEY=your-anon-key

# Chave de serviço (se necessário) para operações administrativas do Supabase
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Configuração do banco de dados
DB_HOST=localhost          # O host do banco de dados
DB_PORT=5432               # A porta do banco de dados (padrão do PostgreSQL)
DB_USER=your-database-user # Nome de usuário do banco de dados
DB_PASSWORD=your-password  # Senha do banco de dados
DB_NAME=your-database-name # Nome do banco de dados

# Configuração de conexão com o banco de dados local (caso você esteja usando localmente)
DB_SSL=false               # Defina como 'true' se usar SSL para a conexão

# URL de envio de email (se necessário para o envio de notificações)
EMAIL_URL=smtp://your-smtp-server.com
EMAIL_USER=your-email-user
EMAIL_PASSWORD=your-email-password
